peline {  
    agent any  
        stages {  
       	    stage("git_checkout") {  
           	    steps {  
           	            sh '''
           	          
              	    git clone https://github.com/sivakiran95/maven-project.git 
              	    echo "repo cloned successfully"
              	   
              	     '''
              	    }  
              	  
         	    } 
         	    
        }
}
